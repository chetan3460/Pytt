target 'Pytt' do
  # Comment the next line if you're not using Swift and don't want to use dynamic frameworks
  use_frameworks!
  pod 'SDWebImage'
  pod 'Cosmos'
  pod 'SwiftLint'
  pod 'SwiftyJSON'
  pod 'SwiftWebVC'
  pod 'Mockingjay'

  target 'PyttTests' do
    inherit! :search_paths
    # Pods for testing
    pod 'Quick'
    pod 'Nimble'
  end

  target 'PyttUITests' do
    inherit! :search_paths
    # Pods for testing
  end

end
